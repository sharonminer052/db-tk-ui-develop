<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
<ui:with field='messages' type='config.i18n.client.ClientMessages'/>
<g:FlowPanel addStyleNames="application-container" ui:field="container">
    <g:FlowPanel addStyleNames="output-format">
        <g:Label addStyleNames="h1">
            <ui:text from='{messages.wizardSendToExportFormatTitle}'/>
        </g:Label>
        <g:Label addStyleNames="h5">
            <ui:text from='{messages.wizardSendToExportFormatSubTitle}'/>
        </g:Label>
        <g:FlowPanel addStyleNames="output-format-buttons" >
            <g:Button addStyleNames="btn btn-save" ui:field="buttonSIARD">
                <ui:text from='{messages.SIARD}'/>
            </g:Button>
            <g:Button addStyleNames="btn btn-table" ui:field="buttonDBMS">
                <ui:text from='{messages.menuSidebarDatabases}'/>
            </g:Button>
        </g:FlowPanel>
    </g:FlowPanel>
</g:FlowPanel>
</ui:UiBinder>