<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:FlowPanel addStyleNames="block" ui:field="content" >
        <g:FlowPanel addStyleNames="wrapper block">
            <g:FlowPanel ui:field="leftSideContainer" addStyleNames="left">
                <g:FlowPanel addStyleNames="block" ui:field="tableAndColumnsList"/>
            </g:FlowPanel>
            <g:FlowPanel addStyleNames="right">
                <g:FlowPanel addStyleNames="block" ui:field="panel" />
            </g:FlowPanel>
        </g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>