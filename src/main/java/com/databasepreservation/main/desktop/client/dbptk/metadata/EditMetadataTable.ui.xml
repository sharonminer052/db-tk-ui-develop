<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field='messages' type='config.i18n.client.ClientMessages' />
    <g:FlowPanel>
        <g:SimplePanel addStyleNames="h1" ui:field="mainHeader">
            <g:Label><ui:text from='{messages.loadingTableInfo}' /></g:Label>
        </g:SimplePanel>
        <g:HTML ui:field="description"/>
<!--        <g:SimplePanel ui:field="tableContainer"  addStyleNames="TableSearchPanel-table"/>-->
        <g:FlowPanel>
            <g:TabPanel ui:field="tabPanel" addStyleNames="browseItemMetadata" />
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>