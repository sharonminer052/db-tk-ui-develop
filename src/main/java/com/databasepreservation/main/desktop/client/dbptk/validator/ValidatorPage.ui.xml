<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:m='urn:import:com.databasepreservation.main.common.shared.client.breadcrumb'
             xmlns:c='urn:import:com.databasepreservation.main.common.shared.client.common'>


    <g:FlowPanel addStyleNames="application-container" ui:field="container">
        <m:BreadcrumbPanel ui:field="breadcrumb"/>
        <g:FlowPanel addStyleNames="browseContentPreviewPanel validation">
            <c:LoadingDiv visible="false" ui:field="loading"/>
            <g:Label ui:field="title" addStyleNames="h1"/>
            <g:FlowPanel ui:field="validationInformation"/>
            <g:FlowPanel>
                <g:FlowPanel ui:field="content" addStyleNames="progress-report validation"/>
            </g:FlowPanel>
            <g:FlowPanel ui:field="validationControl"/>
        </g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>