<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.databasepreservation.common.shared.client.common'>

    <g:FlowPanel addStyleNames="progress-bar-panel">
        <g:Label ui:field="title" addStyleNames="h1"/>
        <g:Label ui:field="subTitle" addStyleNames="h5"/>
        <g:FlowPanel addStyleNames="progress-report-panel">
            <c:Progressbar ui:field="progressBar"/>
            <g:FlowPanel ui:field="content" addStyleNames="progress-report"/>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
