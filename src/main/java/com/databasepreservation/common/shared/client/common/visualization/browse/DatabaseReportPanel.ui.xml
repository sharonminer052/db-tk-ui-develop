<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:wcag='urn:import:com.databasepreservation.common.shared.client.widgets.wcag'
>

    <ui:with field='messages' type='config.i18n.client.ClientMessages' />
    <g:FlowPanel>
        <g:Label addStyleNames="h1"><ui:text from='{messages.titleReport}' /></g:Label>
        <wcag:MarkdownWidgetWrapper ui:field="contentItems"/>
    </g:FlowPanel>
</ui:UiBinder>
