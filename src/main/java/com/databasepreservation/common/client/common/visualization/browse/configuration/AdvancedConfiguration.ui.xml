<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field='messages' type='config.i18n.client.ClientMessages' />
    <g:FlowPanel addStyleNames="advanced-configuration">
        <g:Label ui:field="title" addStyleNames="h1"/>
        <g:FlowPanel ui:field="options" addStyleNames="advanced-configuration-options"/>
    </g:FlowPanel>
</ui:UiBinder>