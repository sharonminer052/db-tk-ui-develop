<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.databasepreservation.common.client.common'>

    <g:FlowPanel addStyleNames="col_12" ui:field="container">
        <g:FlowPanel addStyleNames="browseContentPreviewPanel" ui:field="panel">
            <c:LoadingDiv visible="false" ui:field="loading"/>
            <g:FlowPanel addStyleNames="structure-header-siard" ui:field="mainHeader" />
            <g:SimplePanel ui:field="description"/>
            <g:FlowPanel ui:field="metadataInformation"/>
            <g:FlowPanel addStyleNames="navigation-panels" ui:field="navigationPanels"/>
            <g:FlowPanel addStyleNames="clearfix"/>
            <g:FlowPanel addStyleNames="navigation-panel-buttons">
                <g:SimplePanel addStyleNames="btn-item">
                    <g:Button ui:field="btnDeleteAll" addStyleNames="btn btn-danger btn-delete"/>
                </g:SimplePanel>
                <g:SimplePanel addStyleNames="btn-item">
                  <g:Button ui:field="btnBack" addStyleNames="btn btn-primary btn-back-wizard"/>
                </g:SimplePanel>
            </g:FlowPanel>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
