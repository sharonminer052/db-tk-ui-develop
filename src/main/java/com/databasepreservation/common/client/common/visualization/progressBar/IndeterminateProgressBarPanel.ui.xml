<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:progressBar="urn:import:com.databasepreservation.common.client.common.visualization.progressBar">

    <g:FlowPanel addStyleNames="col_12">
        <g:SimplePanel addStyleNames="structure-header" ui:field="header"/>
        <g:SimplePanel ui:field="description"/>
        <g:FlowPanel addStyleNames="progress-report-panel">
            <g:SimplePanel addStyleNames="progress-line" ui:field="progress"/>
            <g:FlowPanel ui:field="content" addStyleNames="progress-report"/>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
